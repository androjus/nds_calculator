<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="nds_calculator" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="${PROJECT_DIR}" />
		<Option pch_mode="2" />
		<Option compiler="devkitpro_arm_compiler" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/nds_calculator" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="devkitpro_arm_compiler" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile" />
					<CompileFile command="$make -f $makefile" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile clean" />
					<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="Makefile" />
		<Unit filename="hello_world.pnproj" />
		<Unit filename="hello_world.prj" />
		<Unit filename="source/main.cpp" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
